@model List<Sala>
@{
    ViewData["Title"] = "Bioskop-Sale";
}

<h4 class="display-3">Dostupne sale</h4>

<table class="table">
    <thead>
        <tr>

            <th>
                Naziv sale
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Sala s in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => s.NazivSale)
                </td>

                <td>
                    @Html.ActionLink("Obrisi", "Delete", new { id = s.SalaId })
                </td>
            </tr>
        }
    </tbody>
</table>